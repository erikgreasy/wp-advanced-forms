{
    "name": "erikgreasy/wp-advanced-forms",
    "description": "This is my package wp-advanced-forms",
    "keywords": [
        "erikgreasy",
        "wp-advanced-forms"
    ],
    "homepage": "https://github.com/erikgreasy/wp-advanced-forms",
    "license": "MIT",
    "authors": [
        {
            "name": "erikgreasy",
            "email": "erik.masny@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/translation": "^8.0 || ^9.0",
        "illuminate/validation": "^8.0 || ^9.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.0",
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28"
    },
    "autoload": {
        "psr-4": {
            "Erikgreasy\\WpAdvancedForms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Erikgreasy\\WpAdvancedForms\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
